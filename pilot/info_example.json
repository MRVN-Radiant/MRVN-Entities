/* name this file info_example.json to match "Entity" */
{
    "Contributors": ["snake-biscuits"],
    /* Index */
    "Block": "ENTITIES",  /* override mrvn/ENTITES.xml */
    "Entity": "info_example",  /* entity name this file affects */
    /* Overrides, in Order of Appearance */
    "Type": "point",  /* point for point entitiy, group for brush entitiy */
    "Description": "An example point entity",
	"Keys": {"keyname": "name",  /* name of key to override / add */
             /* overrides */
             "type": "targetname",
             "name": "Display Name",
             "description": "Changed."},
    "SpawnFlags": {"bit": 0,  /* Index of bit to override / add*/
                   /* overrides */
                   "name": "NO_RENDER",
                   "default": "True",
                   "description": "Don't render"},
	"Notes": "This goes under the NOTES header & the first line is not overridden"
}
<maxcpulevel key="maxcpulevel" name="Maximum CPU Level" value="0"/>
<mingpulevel key="mingpulevel" name="Minimum GPU Level" value="0"/>
<maxgpulevel key="maxgpulevel" name="Maximum GPU Level" value="0"/>
<boolean key="disableX360" name="Disable X360" value="0"/>
<integer key="maxspeed" name="Max Rotation Speed" value="100">The maximum rotation speed of the spotlight, in degrees per second.</integer>
<integer key="spotlightlength" name="Spotlight Length" value="500">Length of the spotlight beam.</integer>
<integer key="spotlightwidth" name="Spotlight Width" value="50">Width of the spotlight beam.</integer>
